<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chatsystem.View.ChatViewController">
    <children>
        <TextField fx:id="messageField" layoutX="251.0" layoutY="361.0" prefHeight="26.0" prefWidth="242.0" />
        <Button fx:id="sendButton" layoutX="503.0" layoutY="361.0" mnemonicParsing="false" onAction="#onSend" prefHeight="18.0" prefWidth="82.0" text="Send">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="disconnectButton" layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#onDisconnect" text="Disconnect" />
        <Button fx:id="informationButton" layoutX="125.0" layoutY="362.0" mnemonicParsing="false" onAction="#onInformation" text="Information" />
        <Label layoutX="14.0" layoutY="15.0" text="Username: " />
        <Label fx:id="usernameLabel" layoutX="74.0" layoutY="14.0" prefHeight="18.0" prefWidth="114.0" text="&quot;username&quot;" />
        <ListView fx:id="chatArea" layoutX="14.0" layoutY="37.0" prefHeight="316.0" prefWidth="569.0" />
    </children>
</AnchorPane>
